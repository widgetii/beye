;
; Bash syntax definition for biew project
;
#nosmart
[General]
Name=Bash's syntax
WSet=ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz_.-0123456789

[Context]
  <Comments>
    base=#...\n
  <Constants>
    base="..."
    extended='...'
    alt=`...`
    reserved=EOF...EOF

[Keywords]
base=for
base=in
base=do
base=done
base=select
base=case
base=esac
base=if
base=then
base=elif
base=else
base=fi
base=while
base=until

[Operators]
base=$
base=\x3B
